<%- include("partials/header") -%>

    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
        <label for="title">Title</label>
        <input type="text" name="titlePublish" class="form-control" id="" placeholder="Write Here" autocomplete="on">
        <br>
        <label for="content">Content</label>
        <textarea name="postPublish" class="form-control" id="" rows="5" placeholder="Write Here" autocomplete="off"></textarea>
    </div>
        <button type="submit" class="btn btn-primary">Publish</button>
    </form>

    <%- include("partials/footer") -%>